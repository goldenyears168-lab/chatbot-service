{
  "id": "initialize-services",
  "name": "Initialize Services",
  "version": "1.0.0",
  "category": "core",
  "description": "验证服务已初始化（知识库、LLM、上下文管理器）",
  "icon": "⚙️",
  "color": "#FF9800",
  
  "inputs": [
    {
      "name": "knowledgeBase",
      "type": "KnowledgeBase",
      "required": true,
      "description": "知识库实例"
    },
    {
      "name": "llmService",
      "type": "LLMService",
      "required": true,
      "description": "LLM 服务实例"
    },
    {
      "name": "contextManager",
      "type": "ContextManager",
      "required": true,
      "description": "上下文管理器实例"
    },
    {
      "name": "companyId",
      "type": "string",
      "required": true,
      "description": "公司 ID"
    }
  ],
  
  "outputs": [
    {
      "name": "success",
      "type": "object",
      "description": "服务验证成功"
    },
    {
      "name": "error",
      "type": "Error",
      "description": "服务初始化失败"
    }
  ],
  
  "config": {
    "validateKnowledgeBase": true,
    "validateLLMService": true,
    "validateContextManager": true
  }
}
